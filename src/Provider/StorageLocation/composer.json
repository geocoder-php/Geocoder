{
    "name": "geocoder-php/storage-location-provider",
    "type": "library",
    "description": "Geocoder storage location provider",
    "keywords": [],
    "homepage": "http://geocoder-php.org/Geocoder/",
    "license": "MIT",
    "authors": [
        {
            "name": "Borys Yermokhin",
            "email": "borys.ermokhin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/log": "^1.0",
        "willdurand/geocoder": "^4.0",
        "psr/cache": "^1.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.5",
        "php-http/message": "^1.0",
        "php-http/curl-client": "^1.7",
        "nyholm/nsa": "^1.1",
        "geocoder-php/provider-integration-tests": "^1.0",
        "cache/array-adapter": "^1.0"
    },
    "provide": {
        "geocoder-php/provider-implementation": "1.0"
    },
    "autoload": {
        "psr-4": { "Geocoder\\Provider\\StorageLocation\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
