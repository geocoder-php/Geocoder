{
    "name": "geocoder-php/algolia-places-provider",
    "type": "library",
    "description": "Geocoder AlgoliaPlaces adapter",
    "keywords": [],
    "homepage": "http://geocoder-php.org/Geocoder/",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Lalanne",
            "email": "lalanne.fabien@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "geocoder-php/common-http": "^4.1",
        "willdurand/geocoder": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.3.*",
        "geocoder-php/provider-integration-tests": "^1.1",
        "php-http/message": "^1.0",
        "php-http/curl-client": "^1.7",
        "nyholm/psr7": "^0.2.2"
    },
    "provide": {
        "geocoder-php/provider-implementation": "1.0"
    },
    "autoload": {
        "psr-4": { "Geocoder\\Provider\\AlgoliaPlaces\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
